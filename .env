# .env 文件示例
DEBUG=True
PORT=8180
HOST=0.0.0.0
LOG_LEVEL=INFO

# 嵌入向量模型配置
EMBEDDING_MODEL=lightweight
EMBEDDING_DEVICE=cpu

# 语音模型配置
FUN_ASR_MODEL=paraformer-zh
FUN_ASR_DEVICE=cpu

# API密钥
QWEN_API_KEY=your-api-key-here 
QWEN_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation
QWEN_API_MODEL=qwen-turbo

# 模型缓存路径配置
## 可以使用绝对路径
## MODEL_CACHE_PATH=/path/to/your/models
MODEL_CACHE_PATH=./models

# 语义分段中文模型
ZH_CORE_WEB_SM=zh_core_web_sm
